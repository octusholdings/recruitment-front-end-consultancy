    "gauge": "./src/vendors/gauge/gauge.js",
    "bloodhound": "./node_modules/corejs-typeahead/dist/bloodhound.js",
    "stupidTable": "./lib/stupidTable/stupidTable.js",
    "imageViewer": "./lib/imageViewer/imageviewer.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "hbsfy"
    ]
  },
  "browserify-swap": {
    "@packages": [
      "underscore"
    ],
    "all": {}
  },
  "browserify-shim": {
    "jquery": "$",
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "underscore": "_",
    "backbone.stickit": {
      "depends": [
        "backbone"
      ],
      "exports": "Backbone.Stickit"
    },
    "backbone.marionette": {
      "depends": [
        "backbone"
      ]
    },
    "jquery.ui": {
      "depends": [
        "jquery:jQuery"
      ],
      "exports": null
    },
    "easyPieChart": {
      "depends": [
        "jquery:jQuery"
      ],
      "exports": null
    },
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ],
      "exports": "bootstrap"
    },
    "bootstrap-notify": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "bloodhound": {
      "exports": "Bloodhound",
      "depends": "jquery:jQuery"
    }
  },
